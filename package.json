{
  "name": "TodoApp",
  "version": "0.1.0",
  "description": "Todo App",
  "main": "app/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --colors",
    "build:cache": "sw-precache --root=dist",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:copy": "cp -R ./app/static ./dist/static",
    "build:js": "cross-env NODE_ENV=production webpack --progress --colors --display-error-details",
    "build:all": "npm run build:clean && npm run build:copy && npm run build:js && npm run build:cache",
    "build": "npm run build:all",
    "test": "mocha ./tests --require ./helpers/test-helper --compilers js:babel-core/register --recursive"
  },
  "author": "Nikolaev Tomov",
  "license": "",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.18.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.16.0",
    "chai": "^4.0.1",
    "compression-webpack-plugin": "^1.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.3",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.14.0",
    "style-loader": "^0.18.1",
    "sw-precache": "^5.1.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "es6-promise": "^4.1.1",
    "immutable": "^3.8.2",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.17.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.5",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-form": "^7.0.4",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.15.1",
    "reselect": "^3.0.0"
  }
}
